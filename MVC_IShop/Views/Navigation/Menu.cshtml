@model IEnumerable<string>


<h2 class="menuTitle" style="margin-left: -130px;">Категории:</h2>
<h3 class="menu-item">@Ajax.ActionLink("Все", "GetGoodsList", "Goods", new { id = 1, filter = "Все", filterdata = "" }, new AjaxOptions { UpdateTargetId = "goods_list" }, new { onclick = "normalise()" })</h3>
@foreach (var i in Model)
{
    <h3 class="menu-item" name="@i"><a class="filtLink" onclick = @String.Format("filters('{0}')", @i)>@i</a> </h3>
}
